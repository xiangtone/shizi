/* pages/cat-second/cat-second.wxss */

.info {
    width: 100%;
    height: 420rpx;
    position: relative;
}

.info image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
}

.title {
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding: 0 20rpx;
    text-align: center;
    position: relative;
    z-index: 999;
}
