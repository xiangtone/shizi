/* pages/cat-second/cat-second.wxss */
@import "/commons/weui.wxss";
@import "/commons/sc-pub.wxss";
.roomWarn {
  background: red;
  animation: myblink 3s;
  animation-iteration-count: infinite;
}

@keyframes myblink {
  0% {
    background: #f9daa9;
  }
  50% {
    background: yellow;
  }
  100% {
    background: #f9daa9;
  }
}

.info {
  width: 100%;
  height: 420rpx;
  position: relative;
}

.info image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.title {
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  padding: 0 20rpx;
  text-align: center;
  position: relative;
  z-index: 999;
}

.video-one {
  width: 100%;
  padding: 40rpx 32rpx;
}

.video-one .pic {
  width: 220rpx;
  height: 124rpx;
  margin-right: 24rpx;
  background-size: cover;
  background-position: center;
}
