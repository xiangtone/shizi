/* pages/video/video.wxss */
@import "/commons/comment/comment.wxss";
@import "/commons/share/share.wxss";
@import "../../wxParse/wxParse.wxss";
@import "/commons/weui.wxss";
@import "/commons/sc-pub.wxss";

page {
  background-color: #fff;
  /* height: 100%; *//* overflow-y: hidden; */
}

.custom_service {
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  z-index: 1;
  border-radius: 100rpx;
  left: 85%;
  top: 90%;
  border: 1rpx solid red;
}

.pbg {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.pbg image {
  position: fixed;
  height: 100%;
  width: 100%;
}

.video {
  width: 100%;
  height: 520rpx;
  /* position: absolute;
  left: 0;
  top: 0;
  z-index: 99; */
  /* background-color: #fff; */
}

.kong {
  width: 100%;
  height: 520rpx;
}

.video video {
  width: 100%;
  height: 420rpx;
}

.scroll-view-x {
  white-space: nowrap;
}

.scroll-view-x .scroll-view-item {
  display: inline-block;
  height: 150rpx;
  width: 450rpx;
  margin-right: 16rpx;
  position: relative;
}

.coupon {
  height: 100%;
  width: 450rpx;
  position: absolute;
  z-index: -1;
}

.coupon_left {
  float: left;
  margin-top: 15rpx;
  color: #fff;
  width: 230rpx;
}

.coupon_right {
  float: right;
  margin: 25rpx 20rpx 0rpx 0rpx;
  color: #fff;
  width: 150rpx;
}

.sub_price {
  font-size: 23px;
  font-weight: bold;
  margin: 0rpx 0rpx 10rpx 32rpx;
}

.original_cost {
  margin-left: 22rpx;
  font-size: 15px;
}

.coupon_name {
  font-size: 15px;
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.coupon_receive {
  font-size: 14px;
  color: #ff4544;
  background: #fff;
  border-radius: 40rpx;
  width: 155rpx;
  height: 55rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.chakan {
  height: 80rpx;
  color: #ff4544;
  font-size: 13px;
  display: flex;
  align-items: center;
  justify-content: center;
}

#kan {
  display: none;
}

.collect_fees_coupon {
  white-space: nowrap;
  height: 184rpx;
  border-bottom: 1px solid #e2e2e2;
}

.fees_coupon_info {
  white-space: nowrap;
  height: 184rpx;
}

.fees_coupon_name_price {
  float: left;
  margin-left: 24rpx;
}

.fees_coupon_name {
  margin-top: 40rpx;
  color: #303030;
  font-size: 13px;
}

.fees_coupon_price {
  margin-top: 22rpx;
}

.fees_coupon_right {
  float: right;
}

.surplus {
  margin-top: 40rpx;
  font-size: 13px;
  color: #999;
  display: flex;
  align-items: center;
  justify-content: center;
}

.buynow {
  width: 180rpx;
  height: 64rpx;
  border: 1px solid #ff4544;
  border-radius: 32rpx;
  margin-top: 20rpx;
  font-size: 12px;
  color: #ff4544;
  display: flex;
  align-items: center;
  justify-content: center;
}

.buy_now {
  width: 180rpx;
  height: 64rpx;
  border-radius: 32rpx;
  margin-top: 20rpx;
  font-size: 12px;
  color: #fff;
  background: #ccc;
  display: flex;
  align-items: center;
  justify-content: center;
}

.info {
  width: 100%;
  position: relative;
}

.info-title {
  width: 100%;
  height: 120rpx;
  position: relative;
  padding: 0 0rpx;
}

.info-title .right {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 24rpx;
}

.share {
  width: 40rpx;
  height: 38rpx;
}

.collect {
  width: 132rpx;
  height: 80rpx;
  margin-right: 5rpx;
}
.functionBtn {
  width: 100%;
  padding-top:30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.functionBtn image {
  width: 662rpx;
  height: 249rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.shareBtn {
  width: 100%;
  position: fixed;
}

.shareBtn image {
  position: fixed;
  right:0;
  width: 148rpx;
  height: 84rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.share-btn {
  line-height: normal;
  padding: 0 12rpx;
  border: none;
  background: none;
  border-radius: 0;
  box-shadow: none;
  display: inline-block;
  font-size: 8pt;
  margin-top: 15rpx;
}

.share-btn::after {
  content: "";
  border: 0;
}

.info-content {
  color: #999;
  padding: 0 24rpx;
  padding-bottom: 20rpx;
}

.tuijian {
  width: 100%;
  height: 180rpx;
  overflow: auto;
  padding: 0 24rpx;
}

.img {
  width: 304rpx;
  height: 170rpx;
  position: relative;
  margin-right: 14rpx;
}

.img image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}

.info-label {
  width: 100%;
  height: 100%;
  position: relative;
}

.info-label image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 122rpx;
  top: 65rpx;
}

.info-label .title {
  position: absolute;
  bottom: 8rpx;
  left: 6rpx;
  max-width: 200rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #fff;
  font-size: 9pt;
  z-index: 999;
}

.info-label .time {
  position: absolute;
  bottom: 8rpx;
  right: 6rpx;
  max-width: 216rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #fff;
  font-size: 9pt;
  z-index: 999;
}

.Toast {
  position: absolute;
  top: 440rpx;
  left: 255rpx;
  width: 240rpx;
  height: 240rpx;
  background-color: rgba(0, 0, 0, 0.6);
  border-radius: 10rpx;
  z-index: 999;
  text-align: center;
}

.Toast image {
  width: 120rpx;
  height: 84rpx;
  margin-top: 52rpx;
}

.comment-list {
  width: 100%;
  padding: 0 24rpx;
}

.comment-one {
  width: 100%;
  padding: 20rpx 0;
}

.comment-one:last-child {
  padding-bottom: 100rpx;
}

.comment-title {
  width: 100%;
  height: 100rpx;
}

.left {
  width: 80rpx;
  height: 100%;
  margin-right: 12rpx;
}

.left image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 70rpx;
}

.right {
  height: 100%;
  margin-right: 24rpx;
}

.detail {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  background-color: #f2f2f2;
  width: 100%;
  overflow-x: hidden;
}

.detail-content {
  width: 100%;
  padding: 24rpx;
  background-color: #fff;
  box-sizing: border-box;
}

.detail-content text, .detail-content image, .detail-content view {
  box-sizing: border-box;
  max-width: 100%;
}

.detail-title {
  background-color: #fff;
  width: 100%;
  height: 88rpx;
  padding: 0 24rpx;
  color: #999;
}

.detail-title image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 14rpx;
}

.form-list {
  width: 100%;
  background-color: #fff;
  padding: 0 24rpx;
}

.detail-text {
  width: 100%;
  height: 96rpx;
}

.detail-textarea {
  width: 100%;
  padding: 14rpx 0;
  min-height: 96rpx;
}

.detail-btn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  color: #fff;
  font-size: 13pt;
  background-color: #ff5d21;
  background-image: -webkit-linear-gradient(120deg, #ff7835, #ff5d21);
  background-image: linear-gradient(120deg, #ff7835, #ff5d21);
  z-index: 999;
  border-radius: 0;
}

.border-0 {
  border: 0;
}

.cover {
  position: relative;
  width: 100%;
  height: 420rpx;
}

.cover-img {
  width: 100%;
  height: 100%;
}

.cover-play {
  position: absolute;
  top: 146rpx;
  left: 311rpx;
  width: 128rpx;
  height: 128rpx;
}

.page-view {
  /* margin-left: 32rpx; */
}

.page-view image {
  width: 36rpx;
  height: 28rpx;
  margin-right: 16rpx;
}

.bg {
  background-color: #181818;
  padding-top: 128rpx;
  color: #fff;
}

.pay-text {
  margin-bottom: 40rpx;
}

.pay-btn {
  min-width: 272rpx;
  height: 88rpx;
  border-radius: 44rpx;
  padding: 0 20rpx;
}

.btn-1 {
  background-color: #333;
  margin-right: 20rpx;
}

.btn-2 {
  background-color: #df4833;
  color: #fff;
}

.pp {
  position: relative;
  height: 100%;
}

.show_modal {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
  background-color: rgba(0, 0, 0, 0.6);
  height: 100%;
  width: 100%;
}

.show_modal .modal {
  width: 550rpx;
  background-color: #fff;
  border-radius: 16rpx;
}

.modal-body {
  padding: 56rpx 0;
  text-align: center;
  color: #303030;
}

.modal-footer {
  margin-bottom: 64rpx;
}

.pay-btn-1 {
  width: 224rpx;
  height: 72rpx;
  border-radius: 36rpx;
  border: 1rpx solid #df4833;
}

.btn-3 {
  color: #df4833;
  background-color: #fff;
  margin-right: 25rpx;
}

.modal-title {
  width: 100%;
  height: 144rpx;
  border-radius: 16rpx 16rpx 0 0;
  position: relative;
}

.modal-xx {
  position: absolute;
  top: 0;
  right: 0;
  height: 92rpx;
  width: 92rpx;
}

.modal-xx image {
  width: 28rpx;
  height: 28rpx;
}
