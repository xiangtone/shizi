page {
  background-color: #f5f5f5;
}

.login {
  background-color: #fff;
  padding: 20px 20px 50px 20px;
}

.m_label {
  display: inline-block;
  max-width: 425px;
  text-align: left;
  width: 89%;
  font-size: 14px;
  color: #7c7b79;
  padding: 19px 0;
}

.m_button {
  color: #fff;
  margin-top: 15px;
  border-radius: 18px;
  background: #1aad19;
  width: 85%;
}

.bg {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.bg image {
  position: fixed;
  height: 100%;
  width: 100%;
}

.startBtn {
  width: 100%;
  height: 140rpx;
  bottom: 25%;
  position: absolute;
  z-index: 1;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.startBtn image {
  width: 500rpx;
  height: 150rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.frontBtn {
  width: 100%;
  height: 140rpx;
  bottom: 2%;
  position: absolute;
  z-index: 2;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.frontBtn image {
  width: 140rpx;
  height: 140rpx;
}
